#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/minpeter/github.com/minpeter/grpc-web-example/node_modules/.pnpm/serve@11.3.2/node_modules/serve/bin/node_modules:/Users/minpeter/github.com/minpeter/grpc-web-example/node_modules/.pnpm/serve@11.3.2/node_modules/serve/node_modules:/Users/minpeter/github.com/minpeter/grpc-web-example/node_modules/.pnpm/serve@11.3.2/node_modules:/Users/minpeter/github.com/minpeter/grpc-web-example/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/minpeter/github.com/minpeter/grpc-web-example/node_modules/.pnpm/serve@11.3.2/node_modules/serve/bin/node_modules:/Users/minpeter/github.com/minpeter/grpc-web-example/node_modules/.pnpm/serve@11.3.2/node_modules/serve/node_modules:/Users/minpeter/github.com/minpeter/grpc-web-example/node_modules/.pnpm/serve@11.3.2/node_modules:/Users/minpeter/github.com/minpeter/grpc-web-example/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/serve.js" "$@"
else
  exec node  "$basedir/../../bin/serve.js" "$@"
fi
